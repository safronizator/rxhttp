{
  "name": "rxhttp",
  "version": "0.1.0",
  "description": "Lightweight Web framework based on RxJS",
  "main": "index.js",
  "scripts": {
    "test": "npm run compile && npm run mocha",
    "dev": "DEBUG=rxhttp.* ts-node-dev --no-notify src/examples/test-serve.ts",
    "mocha": "ts-mocha \"test/**/*.ts\" --recursive --exit",
    "compile": "shx rm -rf dist/ && tsc",
    "dist-make": "tsc --outDir . && shx rm -rf examples/",
    "dist-clean": "shx rm -rf *.d.ts *.js ext/ tsconfig.tsbuildinfo",
    "prepublishOnly": "npm run dist-make",
    "postpublish": "npm run dist-clean"
  },
  "publishConfig": {
    "registry":"https://npm.pkg.github.com/"
  },
  "homepage": "https://github.com/safronizator/rxhttp",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/safronizator/rxhttp.git"
  },
  "keywords": [
    "http",
    "rx",
    "rxjs",
    "web",
    "framework"
  ],
  "author": "safronizator <me@safron.pro>",
  "license": "ISC",
  "dependencies": {
    "debug": "^4.1.1",
    "find-my-way": "^2.2.1",
    "rxjs": "^6.5.4"
  },
  "devDependencies": {
    "@types/debug": "^4.1.5",
    "@types/mocha": "^7.0.1",
    "@types/node": "^13.7.4",
    "@types/supertest": "^2.0.8",
    "mocha": "^7.1.0",
    "shx": "^0.3.2",
    "supertest": "^4.0.2",
    "ts-mocha": "^6.0.0",
    "ts-node-dev": "^1.0.0-pre.44",
    "typescript": "^3.8.2"
  }
}
